#include <stdio.h>

int main()
{
    int num,i;
    printf("enter number: ");
    scanf("%d",&num);
    for(i=2;i<=num-1;i++){
        if(num%i ==0){
            printf("number is not prime number");
            break;
        }
    }
    if(num ==i){
        printf("number is prime number");
    }

    return 0;
}
