#include <stdio.h>

int main()
{
    int i,x;
    printf("enter number:");
    scanf("%d",&x);
    for(i=1;i<x;i++){
        if (x%2==0){
            printf("number is even");
            break;
        }
        else
        { printf("number is odd");
        break;
        }
    }

    return 0;
}

