#include <stdio.h>

void main()
{
    int a,b,c;
    printf("enter three numbers: \n");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b && a>c){
        printf("%d is greatest amidst three",a);
    }
    if(b>a && b>c){
        printf("%d is greatest in three ",b);
    }
    else
    printf("%d is greatest amidst three ",c);
}
